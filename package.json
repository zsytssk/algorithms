    "eslint-plugin-react-hooks": "^4.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.14.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-unicorn": "^42.0.0",
    "prettier": "^2.8.7",
    "typescript": "^4.2.4"
